<%- include("../template/navBar") %>

<body>
  <br>
  <div class="form-register">
      <h2>Ingresar Medicamento</h2>
      <hr>
    <br>
    <form action="./addMedica" method="POST" class="container-form">
      <div class="container-input">
        <div class="input">
          <label for="mediname" class="form-label">Nombre de medicamento</label>
          <input type="controls" name="mediname" id="mediname" class="form-control">
        </div>
      
      <br>
      
        <div class="input">
          <label for="tipo" class="form-label">Tipo</label>
          <input type="controls" name="tipo" id="tipo" class="form-control">
        </div>
      </div>
      <div class="container-input">
        <div class="input">
          <label for="dosis" class="form-label">Dosis</label>
          <input type="controls" name="dosis" id="dosis" class="form-control">
        </div>
      <br>
        <div class="input">
          <label for="precio" class="form-label">Precio</label>
          <input type="controls" name="precio" id="precio" class="form-control">
        </div>
      </div>
      <div class="container-input">
        <div class="input">
          <label for="venciomiento" class="form-label">Vencimiento</label>
          <input type="date" name="vencimiento" id="vencimiento" class="form-control">
        </div>
       </div>
      <div class="container-btn">
       
          <div class="float-start">
            <button type="submit" class="boton">Agregar</button>
          </div>
        </div>
        
      </div>
    </form>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4"
    crossorigin="anonymous"></script>
  <script src="js/script.js"></script>
  <script src="js/menu.js"></script>

</body>

</html>